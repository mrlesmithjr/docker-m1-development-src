{
	"name": "docker-m1-development",
	"build": {
		"args": {
			"IMAGE": "mrlesmithjr/debian:11",
			"USER_GID": "1000",
			"USER_UID": "1000",
			"USERNAME": "vscode",
		},
		"dockerfile": "../Dockerfile",
	},
	"remoteUser": "vscode",
	"runArgs": [
		"-v",
		"${localEnv:HOME}/.ssh:/home/vscode/.ssh:rw"
	],
	"settings": {},
	"extensions": [
		"github.vscode-pull-request-github",
		"eamodio.gitlens"
	]
}
